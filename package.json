        "@typescript-eslint/eslint-plugin": "5.29.0",
        "@typescript-eslint/parser": "5.29.0",
        "builtin-modules": "3.3.0",
        "esbuild": "0.14.47",
        "obsidian": "latest",
        "prettier": "^2.8.1",
        "tslib": "2.4.0",
        "typescript": "4.7.4"
    },
    "dependencies": {
        "fabric": "^5.2.4"
    }
